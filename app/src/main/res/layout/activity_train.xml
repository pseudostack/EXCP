<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:bootstrap="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#ffffffff"
    tools:context="com.example.farhan.excersie.Train">

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ffffffff"
        android:id="@+id/frameLayout2">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:id="@+id/next">

            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="350dp"
                android:layout_row="7"
                android:layout_column="0"
                android:layout_rowSpan="6"
                android:layout_columnSpan="3"
                android:divider="@android:color/holo_blue_bright">

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="40dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Set 1 (lbs)"
                        android:id="@+id/textView4"
                        android:layout_column="0"
                        android:layout_row="7"
                        android:layout_marginRight="5dp"
                        android:textSize="16dp" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="40dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Set 2 (lbs)"
                        android:id="@+id/textView6"
                        android:layout_column="1"
                        android:textSize="16dp"
                        android:layout_marginRight="5dp" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="40dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Set 3 (lbs)"
                        android:id="@+id/textView7"
                        android:layout_column="3"
                        android:textSize="16dp"
                        android:layout_marginRight="5dp" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="40dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Set 4 (lbs)"
                        android:id="@+id/textView8"
                        android:layout_column="4"
                        android:textSize="16dp"
                        android:layout_marginRight="5dp" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="40dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Set 5 (lbs)"
                        android:id="@+id/textView9"
                        android:layout_column="5"
                        android:textSize="16dp"
                        android:layout_marginRight="5dp"
                        android:textAlignment="center" />

                </TableRow>

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:clickable="false">

                    <NumberPicker
                        android:layout_width="65dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/numberPicker"
                        android:layout_column="0"
                        android:measureWithLargestChild="true"/>

                    <NumberPicker
                        android:layout_width="65dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/numberPicker2"
                        android:layout_column="1"/>

                    <NumberPicker
                        android:layout_width="65dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/numberPicker3"
                        android:layout_column="3"/>

                    <NumberPicker
                        android:layout_width="65dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/numberPicker4"
                        android:layout_column="4"/>

                    <NumberPicker
                        android:layout_width="65dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/numberPicker5"
                        android:layout_column="5"
                        android:clickable="true" />

                </TableRow>


                <com.beardedhen.androidbootstrap.BootstrapButton

                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_margin="10dp"
                    android:text="Next Workout"

                    android:textSize="25sp"
               bootstrap:bootstrapBrand="success"
                     bootstrap:roundedCorners="true" 
                    android:layout_row="15"
                    android:layout_column="0"
                    android:id="@+id/button_next_exercise" />

                <com.beardedhen.androidbootstrap.BootstrapButton

                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_margin="10dp"
                    android:text="Finish Training"

                    android:textSize="25sp"
               bootstrap:bootstrapBrand="success"
                     bootstrap:roundedCorners="true" 
                    android:layout_row="15"
                    android:layout_column="0"
                    android:id="@+id/finish_train" /><![CDATA[


                >

            ]]>
            </TableLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/ex_title"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_columnSpan="2" />

            <WebView
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:id="@+id/ex_demons"
                android:layout_row="1"
                android:layout_column="0"
                android:textAlignment="center"
                android:layout_rowSpan="6" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Personal Best"
                android:id="@+id/textView19" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/txt_best"
                android:text="N/A"
                android:layout_row="2"
                android:layout_column="1"
                android:textSize="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Last Week:"
                android:id="@+id/textView11"
                android:layout_row="3"
                android:layout_column="1"
                android:textSize="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/txt_lastweek"
                android:text="N/A"
                android:layout_row="4"
                android:layout_column="1"
                android:textSize="10sp" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Steps"
                android:id="@+id/btn_Steps"
                android:layout_row="5"
                android:layout_column="1" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tip"
                android:id="@+id/btn_trips"
                android:layout_row="6"
                android:layout_column="1" />

        </GridLayout>
    </FrameLayout>

</RelativeLayout>